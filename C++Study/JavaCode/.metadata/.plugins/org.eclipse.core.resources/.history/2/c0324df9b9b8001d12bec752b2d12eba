package chapter07object;

public class peach {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int day = 1;
		int num = pp(day);
		System.out.println(day);
		
	}
	
	public static int pp(int day) {
		if(day == 10) {
			return 1;
		}else if(day>=1 && day<=9) {
			return ((pp(day+1) + 1) * 2);
		}else {
			return -1;
		}
	}

}
