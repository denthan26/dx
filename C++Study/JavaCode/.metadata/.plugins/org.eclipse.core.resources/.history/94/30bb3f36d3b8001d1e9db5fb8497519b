package chapter07object;

public class MiGong {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//new 数组map
		int[][] map = new int[8][7];
		
		//四周为1
		for(int i = 0; i < 7; i++) {
			map[0][i] = 1;
			map[7][i] = 1;
		}
		
		// 输出数组
		for(int i = 0; i < map.length(); i++) {
			for(int j = 0; j < map[i].length(); j++) {
				System.out.print(map[i][j] + '\t');
			}
			System.out.println();
		}
		

	}

}
